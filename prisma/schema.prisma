datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id          String @id @default(auto())     @map("_id") @db.ObjectId
  nome String
  cpf String
  email String
  telefone String
  senha String
}

model codeSession{
  id      String @id @default(auto())     @map("_id") @db.ObjectId
  email   String
  codigo  String
  date    DateTime
}
